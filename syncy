
config syncy
	option syncylog ''
	option blocksize '10'
	option ondup 'overwrite'
	option datacache 'on'
	option excludefiles '*/Thumbs.db'
	option listnumber '100'
	option retrytimes '3'
	option retrydelay '3'
	option maxsendspeed '0'
	option maxrecvspeed '0'
	option speedlimitperiod '0-0'
	option syncperiod '0-24'
	option syncinterval '3600'
	option tasknumber '3'
	option threadnumber '50'

config syncpath
	option localpath '/download'
	option remotepath '/apps/SyncY/Syncy-docker'
	option enable '1'
	option synctype 'sync'
	option encryption '0'	# 0 is disable encryption
	option encryptkey 'This is a encrypt key'

config syncytoken
